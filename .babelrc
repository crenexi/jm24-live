{
  "env": {
    "development": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        ["@babel/plugin-transform-runtime", {
          "regenerator": true
        }]
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": [
        ["@babel/plugin-transform-runtime", {
          "regenerator": true
        }]
      ]
    }
  }
}
